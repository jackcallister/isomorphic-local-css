  "scripts": {
    "start": "npm run webpack & npm run serve",
    "webpack": "npm run webpack:client & npm run webpack:server",
    "webpack:client": "node_modules/.bin/webpack --watch --verbose --colors --display-error-details --config ./webpack/client.config.js",
    "webpack:server": "node_modules/.bin/webpack --watch --verbose --colors --display-error-details --config ./webpack/server.config.js",
    "postinstall": "node_modules/.bin/webpack --verbose --colors --display-error-details --config ./webpack/server.config.js",
    "serve": "NODE_ENV=development node_modules/.bin/nodemon -q server/boot.js"
  },
